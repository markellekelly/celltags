{
  "name": "celltags",
  "version": "0.1.0",
  "description": "Extension for adding descriptive tags to notebook cells",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/markellekelly/celltags.git",
  "bugs": {
    "url": "https://github.com/markellekelly/celltags.git/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Markelle Kelly",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "static/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/markellekelly/celltags.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0-alpha.6",
    "@jupyterlab/notebook": "^1.0.0-alpha.6",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "rimraf": "^2.6.2",
    "typescript": "~3.5.1"
  },
  "jupyterlab": {
    "extension": true
  }
}
